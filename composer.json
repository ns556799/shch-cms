{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.3",
		"silverstripe/cms": "3.6.1",
		"silverstripe/framework": "3.6.1",
		"silverstripe/reports": "3.6.1",
		"silverstripe/siteconfig": "3.6.1",
	  	"silverstripe-themes/simple": "3.1.*",
		"burnbright/silverstripe-externalurlfield": "*@stable",
		"undefinedoffset/sortablegridfield": "1.0.0",
		"bummzack/sortablefile": "~1.0@dev",
	  	"jonom/silverstripe-betternavigator": "3.0.1",
		"silverstripe/userforms": "~3.1@dev",
		"silverstripe/blog": "~2.2@dev",
	  	"kinglozzer/metatitle": "^1.0",
	  	"silverstripe/googlesitemaps": "^1.4",
		"gdmedia/ss-auto-git-ignore": "^1.0",
		"colymba/gridfield-bulk-editing-tools": "2.1.x",
		"silverstripe/secureassets": "dev-master",
		"silverstripe/widgets":  "^1.2",
		"briceburg/silverstripe-pickerfield": "dev-master",
		"sheadawson/silverstripe-blocks": "^1.1"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7@stable"
	},
	"extra": {
	  "branch-alias": {
		"3.x-dev": "3.6.x-dev"
	  }
	},
	"config": {
		"process-timeout": 600
	},
	"scripts": {
     		"post-update-cmd": "GDM\\SSAutoGitIgnore\\UpdateScript::Go"
	},	
	"prefer-stable": true,
	"minimum-stability": "dev"
}
